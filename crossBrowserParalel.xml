<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="cross Browser" parallel="tests">
    <test name="cross Browser chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="crossBrowser.PositiveLoginTest"></class>
        </classes>
    </test>
    <test name="cross Browser firefox">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="crossBrowser.PositiveLoginTest"></class>
        </classes>
    </test>
    <test name="cross Browser safari">
        <parameter name="browser" value="safari"></parameter>
        <classes>
            <class name="crossBrowser.PositiveLoginTest"></class>
        </classes>
    </test>
</suite>